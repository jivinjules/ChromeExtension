{
  "manifest_version": 2,
  "name": "Jump",
  "description": "Bitcoin Cat Jump",
  "version": "1.0",
  "author": "TheRandomOne",
  "browser_action": {
    "default_icon": "BitcoinCatJumpLogo.png",
    "default_popup": "popup.html",
    "default_title": "Bitcoin Cat Jump"
  },
  "content_security_policy": "script-src 'self' https://tippin.me/buttons/tip.js https://ajax.googleapis.com/ajax/libs/jquery/3.1.0/jquery.min.js; object-src 'self'",
  "permissions": [
    "activeTab"
  ]
}